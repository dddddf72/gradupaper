.content{
    background:linear-gradient(130deg,rgb(250, 224, 255),rgb(255,255,255));
    width: 100%;
    display: grid;
    padding: 25rpx 0;
    grid-template-columns: repeat(auto-fill,250rpx);
    
}
.content>view{
    position: relative;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.content>view:not(:nth-child(-n+3)){
    margin-top: 25rpx;
}

.content>view>image{
    width: 200rpx;
    height: 200rpx;
    border-radius: 20rpx;
}

