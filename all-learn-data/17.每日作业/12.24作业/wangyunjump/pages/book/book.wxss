/* 导航部分开始 */
.header{
    position: relative;
    display: grid;
    grid-template-columns: 1fr  2fr;
    background-color: rgb(166, 172, 87);
    align-items: center;
    justify-items: center;
    padding: 20rpx 0;
}
.txtbtn{
    display: flex;
    align-items: center;
    justify-content: center;
}
.searchbtn{
    background-color: #333;
}
.searchtxt{
    width: 250rpx;
    height: 52rpx;
    margin: 0;
    padding: 0;
    background-color: #fff;
}
.fdj{
    position: absolute;
    top: 28rpx;
    right: 118rpx;
    color: #fff;
    width: 40rpx;
    height: 40rpx;
}
/* 导航部分结束 */

/* 内容部分开始 */
.content{
    width: 100%;
    display: flex;
    align-items: center;
}
.xuhao{
    margin:30rpx 0 0 45rpx;
}
.contentimg{
    margin:30rpx 0 0 25rpx;
    width: 200rpx;
    height: 200rpx;
}
.contenttxt{
    margin:30rpx 0 0 25rpx;
    display: flex;
    flex-direction: column;
}
.contenttxt text{
    margin-top: 15rpx;
}
.title{
    color:rgb(51, 119, 170) ;
}
.rating{
    color: orange;
}
.ratevaluate{
    font-size: 20rpx;
}
.labels{
    font-size: 20rpx;
}
image{
    width: 50rpx;
    height: 50rpx;
}
/* 内容部分结束 */